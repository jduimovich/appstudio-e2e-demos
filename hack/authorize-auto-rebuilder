RT=$(kubectl get routes auto-rebuild -o jsonpath={.spec.host} 2> /dev/null)  
if [ -z $RT ]; then
    echo "rebuilder route not ready" 
else  
    echo "$RT is  ready" 
    echo "curl -k https://$RT/authorize?id=$(oc whoami --show-token)" 
    curl -k https://$RT/authorize?id=$(oc whoami --show-token)
fi